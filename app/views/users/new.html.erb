<h1>Sign Up</h1>


<%= form_for @user, url: signup_path do |f| %>

    <%=f.label "Island Name"%>
    <%=f.text_field :island_name%>

    <%=f.label "Username"%>
    <%=f.text_field :username %>

    <%=f.label "Email"%>
    <%=f.text_field :email %>

    <%=f.label "Dream Address"%>
    <%=f.text_field :dream_address %>

    <%=f.label "Password"%>
    <%=f.password_field :password %>
    <%=f.label "Password Confirmation"%>
    <%=f.password_field :password_confirmation %>
    <%=f.submit "Sign Up"%>
  <%end%>